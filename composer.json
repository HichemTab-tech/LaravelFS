{
    "name": "hichemtab-tech/laravelfs",
    "description": "Laravel application installer with full starter-kits including breeze and jetstream.",
    "keywords": [
        "laravel",
        "installer",
        "starter-kit",
        "breeze",
        "jetstream",
        "laravelfs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hichem Taboukouyout",
            "email": "hichem.tab2002@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0",
        "symfony/polyfill-mbstring": "^1.31"
    },
    "require-dev": {
        "pestphp/pest": "^2.36",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/laravelfs"
    ],
    "autoload": {
        "psr-4": {
            "HichemTabTech\\LaravelFS\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HichemTabTech\\LaravelFS\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
